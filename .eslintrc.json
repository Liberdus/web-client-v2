{
  "env": {
    "browser": true,
    "es2021": true
  },
  "globals": {
    "network": "readonly",
    "QRCode": "readonly",
    "qr": "readonly"
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "prettier/prettier": "error"
  }
}
